{
  "manifest_version": 3,
  "name": "Superpower ChatGPT",
  "version": "5.1.4",
  "key": "",
  "description": "ChatGPT with superpowers! Sync/search history locally, create folders, export all chats, pin messages, access thousands of prompts",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": ["scripting", "storage", "unlimitedStorage", "webRequest"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      }
    }
  },
  "host_permissions": ["https://chat.openai.com/*"],
  "content_scripts": [
    {
      "js": [
        "src/thirdParty/FileSaver.js",
        "src/thirdParty/jszip.js",
        "src/thirdParty/katex.js",
        "src/thirdParty/texmath.js",
        "src/thirdParty/markdown-it-sup.js",
        "src/thirdParty/markdown-it.js",
        "src/thirdParty/turndown.js",
        "src/thirdParty/highlight.js",
        "src/thirdParty/sortable.js",
        "src/thirdParty/sse.js",
        "src/content/contentMessageListeners.js",
        "src/content/rowAssistant.js",
        "src/content/rowUser.js",
        "src/content/quickAccessMenu.js",
        "src/content/newChatPage.js",
        "src/content/promptChain.js",
        "src/content/keyboardShortcuts.js",
        "src/content/modelSwitcher.js",
        "src/content/enableMFA.js",
        "src/content/customInstructions.js",
        "src/content/pluginsDropdown.js",
        "src/content/dropdown.js",
        "src/content/templateWordsModal.js",
        "src/content/shareModal.js",
        "src/content/feedbackModal.js",
        "src/content/toneList.js",
        "src/content/writingStyleList.js",
        "src/content/languageList.js",
        "src/content/reportReasonList.js",
        "src/content/categoryList.js",
        "src/content/sortByList.js",
        "src/content/api.js",
        "src/content/global.js",
        "src/content/instructions.js",
        "src/content/navbar.js",
        "src/content/modal.js",
        "src/content/releaseNote.js",
        "src/content/timestamp.js",
        "src/content/continue.js",
        "src/content/pluginStore.js",
        "src/content/syncBanner.js",
        "src/content/conversationElement.js",
        "src/content/folderElement.js",
        "src/content/clearConversations.js",
        "src/content/export.js",
        "src/content/copyAndCounter.js",
        "src/content/addToPromptLibrary.js",
        "src/content/promptLibrary.js",
        "src/content/promptHistory.js",
        "src/content/settings.js",
        "src/content/autoSave.js",
        "src/content/regenerateResponse.js",
        "src/content/stopGeneratingResponse.js",
        "src/content/conversation.js",
        "src/content/conversationList.js"
      ],
      "css": ["src/styles/global.css", "src/styles/katex.css"],
      "matches": ["https://chat.openai.com/*"]
    }
  ],
  "background": {
    "service_worker": "src/background/background.js"
  },
  "web_accessible_resources": [
    {
      "matches": ["https://chat.openai.com/*"],
      "resources": ["icons/*", "src/content/*", "v2/*"]
    }
  ]
}